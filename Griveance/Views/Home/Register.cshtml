                  }

              });
        </script>
        <script type="text/javascript">
              $('#select').on('change', function () {
                  if (this.value == 'Staff') {
                      $("#designation").show();
                      $("#course").show();

                  } else {
                      $("#designation").hide();
                      $("#course").hide();

                  }

              });
        </script>*@
    <!--end::Global Theme Bundle -->
</body>

<!-- end::Body -->
</html>