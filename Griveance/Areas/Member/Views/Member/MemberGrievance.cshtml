
@{
    ViewBag.Title = "MemberGrievance";
    Layout = "~/Areas/Member/Views/Shared/MemberDashboard.cshtml";
}

<script type="text/javascript" src="~/Content/assets/plugins/jquery/dist/jquery.min.js"></script>
<script type='text/javascript' src='~/Content/assets/app/datatables/jquery.dataTables.min.js'></script>
<script src="@Url.Content("~/Content/angularjs/scripts/angular.min.js")" type="text/javascript"></script>
<script type='text/javascript' src='~/Content/assets/app/datatables/angular-dataTables.js'></script>
<script src="@Url.Content("~/Scripts/ProjectScript/Parent/ParentModule.js")"></script>
<script src="@Url.Content("~/Scripts/Common/Service.js")" type="text/javascript"></script>
<script src="@Url.Content("~/Scripts/ProjectScript/Parent/ParentController.js")" type="text/javascript"></script>

<html lang="en" class="no-js">

<body ng-app="GR">
    @using System.Configuration;
    <div ng-controller="ParentController">
        
        <form name="form" class="student-admission-wrapper" ng-init="GetMember()">
            <div class="m-portlet m-portlet--tab">

                <div class="m-portlet m-portlet--mobile">
                    <div class="m-portlet__head">
                        <div class="m-portlet__head-caption">
                            <div class="m-portlet__head-title">
                                <h3 class="m-portlet__head-text">
                                   List Of Member Grievance
                                </h3>
                            </div>
                        </div>

                    </div>

                </div>
                <div ng-show="IsVisible">
                    <div class="form-body">
                        <div class="m-portlet__body">



                            <div>
                                <div class="form-group m-form__group">
                                    <label>
                                        Complaint Id
                                        <span class="required"> * </span>
                                    </label>

                                    <input type="text" value={{CompID}} ng-model="CompID" name="CompID" class="form-control" required />

                                </div>
                                <div class="form-group m-form__group">
                                    <label>
                                        Grievance Type
                                    </label>

                                    <input type="text" value={{GrievanceType}} ng-model="GrievanceType" name="GrievanceType" class="form-control" disabled required />


                                </div>

                                <div class="form-group m-form__group">
                                    <label>
                                        Subject
                                    </label>

                                    <input type="text" value={{Subject}} name="Subject" ng-model="Subject" class="form-control" disabled required />


                                </div>


                                <div class="form-group m-form__group">
                                    <label>
                                        Post
                                    </label>

                                    <input type="text" value={{Post}} name="Port" ng-model="Post" class="form-control" required />


                                </div>

                                <div class="form-group m-form__group">
                                    <label>
                                        Grievance Action
                                    </label>

                                    <input type="text" value={{GrievanceAction}} ng-model="GrievanceAction" name="Port" class="form-control" required />


                                </div>

                                <div class="form-group m-form__group">
                                    <label>
                                        Status
                                    </label>

                                    <select class="form-control m-input" id="select" ng-model="status" required>
                                      
                                        <option>Pending</option>
                                        <option>Closed</option>
                                        <option>Inprocess</option>
                                   

                                    </select>

                                </div>

                            </div>

                      
                        <div class="m-portlet__foot m-portlet__foot--fit">
                            <div class="m-form__actions">



                                <button class="btn btn-success " ng-show="btnUpdate" ng-click="UPDATE()">
                                    <i class="fa fa-database"> </i>Update
                                </button>
                                <button class="btn default" ng-click="Clear()">
                                    <i class="fa fa-times"> </i> Cancel
                                </button>

                            </div>
                            </div>
                        </div>
                    </div>

                    <!--end::Form-->
                </div>

                <div class="m-portlet__body">
                    <table datatable="ng" dt-options="dtOptions" class="table table-striped- table-bordered  " id="atable">
                        <thead>
                            <tr>
                                <th>Complaint Number</th>
                                <th>Grievance Type</th>
                                <th>Date</th>
                                <th>Subject</th>
                                <th>Post</th>
                                <th>Grievances Action</th>
                                <th>Status</th>
                                <th>Action</th>
                            </tr>
                        </thead>
                        <tbody>
                            <tr ng-repeat="data in Student | filter:search">
                                <td>
                                    {{data.CompID}}
                                </td>
                                <td>
                                    {{data.GrievanceType}}
                                </td>
                                <td>
                                    {{data.Date}}
                                </td>
                                <td>
                                    {{data.Subject}}
                                </td>
                                <td>
                                    {{data.Post}}
                                </td>
                                <td>
                                    {{data.GrievanceAction}}
                                </td>
                                @*<td ng-show="status2">
                            <span ng-style="{'color: red': data.GrievanceAction == 'Closed'}">{{data.GrievanceAction}}</span>
                        </td>*@
                                <td>

                                    {{data.status}}
                                </td>
                              
                                <td><input type="button" class="btn btn-primary" ng-click="ShowHide2(data)" ng-disabled="{{data.status=='Closed'}}" value="Edit"/></td>



                            </tr>
                        </tbody>
                    </table>
                </div>
                </div>
            
        </form>
    </div>

    <script>
        var baseURL = "@ConfigurationManager.AppSettings["BaseUrlPath"].ToString()";
    </script>
</body>
</html>
<style>
    .Error {
        color: red;
    }

    .Warning {
        color: blue;
    }

    .Ignored {
        color: green;
    }
</style>

