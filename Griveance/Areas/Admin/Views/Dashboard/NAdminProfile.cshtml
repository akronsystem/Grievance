    <script>
        var acc = document.getElementsByClassName("accordion");
        var i;
        var x = document.getElementById('span1'); 
        var y = document.getElementById('span2'); 
        for (i = 0; i < acc.length; i++) {
            acc[i].addEventListener("click", function () {
                this.classList.toggle("active");
               
                var panel = this.nextElementSibling;
                if (panel.style.display === "block") {
                    panel.style.display = "none";
                  
                    if (y.classList.value == " fa fa-minus") {
                        y.classList.remove("fa-minus");
                        y.classList.add("fa-plus");
                    }
                    else {
                        x.classList.remove("fa-minus");
                        x.classList.add("fa-plus");
                    }
                  
                } else {
                    panel.style.display = "block";
                    if (x.classList.value == "fa fa-plus") {
                        x.classList.remove("fa-plus");
                        x.classList.add("fa-minus");
                    }
                    else {
                        y.classList.remove("fa-plus");
                        y.classList.add("fa-minus");
                    }
                   
                   
                }
            });
        }
    </script>
   
    <style>
